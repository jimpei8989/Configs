# Setting PATH for Python 3.6
# The original version is saved in .bash_profile.pysave

export PATH="/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/Library/Frameworks/Python.framework/Versions/3.6/bin:/Library/TeX/texbin:/opt/local/bin:/opt/local/sbin:/opt/X11/bin"

